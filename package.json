{
	"name": "vscode-ets",
	"type": "module",
	"displayName": "EJS/ETS language support",
	"description": "EJS/ETS language support for Visual Studio Code.",
	"version": "1.3.1",
	"license": "MIT",
	"icons": "/dist/assets/icon.png",
	"publisher": "leonzalion",
	"author": "Leon Si <contact@leonzalion.com>",
	"engines": {
		"vscode": "^1.62.0"
	},
	"scripts": {
		"build": "node-ts ./scripts/build.ts"
	},
	"categories": [
		"Programming Languages"
	],
	"keywords": [
		"EJS",
		"ETS",
		"embedded javascript",
		"javascript",
		"embedded typescript",
		"typescript",
		"syntax highlighting"
	],
	"contributes": {
		"grammars": [
			{
				"scopeName": "text.html.ets",
				"path": "./dist/syntaxes/ets.tmLanguage.json",
				"injectTo": [
					"text.html"
				],
				"embeddedLanguages": {
					"meta.embedded.block.ets": "ets",
					"meta.embedded.block.typescript": "source.ts"
				}
			}
		],
		"snippets": [
			{
				"language": "html",
				"path": "./snippets/snippets.json"
			}
		]
	},
	"bugs": {
		"url": "https://github.com/leonzalion/vscode-ets/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/leonzalion/vscode-ets"
	},
	"homepage": "https://github.com/leonzalion/vscode-ets#readme",
	"devDependencies": {
		"@leonzalion/configs": "^1.1.11",
		"@types/node": "^17.0.21",
		"@types/sharp": "^0.30.0",
		"chalk": "^5.0.1",
		"desm": "^1.2.0",
		"escape-string-regexp": "^5.0.0",
		"eslint": "^8.11.0",
		"eslint-define-config": "^1.3.0",
		"lion-system": "^1.0.15",
		"outdent": "^0.8.0",
		"prettier": "^2.5.1",
		"sharp": "^0.30.3",
		"typescript": "^4.6.2"
	},
	"prettier": "@leonzalion/configs/prettier.cjs",
	"commitlint": {
		"extends": [
			"@leonzalion/configs/commitlint.cjs"
		]
	}
}
